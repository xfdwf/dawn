content .bottom-button-wrapper button {\n\t\t\tmargin: 0 6px;\n\t\t\tpadding: 6px 0;\n\t\t\tborder-radius: 4px !important;\n\t\t\twidth: 150px;\n\t\t\tmin-width: auto;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .colorStyle .mr10 {\n\t\t\tmargin-right: 10px;\n\t\t}\n\n\t\t.pc-page-tab-button {\n\t\t\tfont-size: 14px;\n\t\t\tcursor: pointer;\n\t\t}\n\n\t\t.pc-page-tab .active {\n\t\t\tfont-weight: 700;\n\t\t\tborder-bottom: 2px solid #171719!important;\n\t\t}\n\n\t\t.btnTab {\n\t\t\tdisplay: none;\n\t\t}\n\n\t\t@media only screen and (max-width: 1024px) {\n\t\t\t#shopify-section-template--16964903600304__section_custom_html_Tp6C9h {\n\t\t\t\ttop: 20px;\n\t\t\t}\n\n\t\t\t.pc-page-tab-content .pc-page-tab-content-left {\n\t\t\t\toverflow-x: auto;\n\t\t\t\toverflow-y: hidden;\n\t\t\t}\n\n\t\t\t.pc-page-tab .pc-page-tab-content .pc-page-tab-content-left>div {\n\t\t\t\tflex: 0 0 33.33%;\n\t\t\t\tmargin: 0;\n\t\t\t\tfont-size: 12px;\n\t\t\t}\n\n\t\t\t.pc-page-tab .pc-page-tab-content {\n\t\t\t\tposition: relative;\n\t\t\t}\n\n\t\t\t.btnTab {\n\t\t\t\tdisplay: block;\n\t\t\t\tposition: absolute;\n\t\t\t\ttop: 50%;\n\t\t\t\ttransform: translateY(-50%);\n\t\t\t\tpadding: 10px;\n\t\t\t\tbackground-color: #f5f5f5;\n\t\t\t\tcolor: #171719;\n\t\t\t\tborder: none;\n\t\t\t\tcursor: pointer;\n\t\t\t\tz-index: 1;\n\t\t\t\twidth: 10px;\n\t\t\t\tdisplay: flex;\n\t\t\t\tjustify-content: center;\n\t\t\t}\n\n\t\t\t.btn-left {\n\t\t\t\tleft: 0;\n\t\t\t}\n\n\t\t\t.btn-right {\n\t\t\t\tright: 0;\n\t\t\t}\n\n\t\t\t.pc-page-tab-content-left::-webkit-scrollbar {\n\t\t\t\tdisplay: none;\n\t\t\t}\n\t\t}\n\t<\/style>\n<\/head>\n\n<body>\n\t<div class=\"page-tab\">\n\t\t<div class=\"pc-page-tab\">\n\t\t\t<div class=\"pc-page-tab-content wrapper\">\n\t\t\t\t<div class=\"pc-page-tab-content-left\">\n\t\t\t\t\t<div class=\"pc-page-tab1 pc-page-tab-button active\" onclick=\"handlePreviewClick()\">\n\t\t\t\t\t\tLOW BACK BODYSUITS\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab2 pc-page-tab-bu
        "custom_code": "<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n\t<meta charset=\"UTF-8\">\n\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n\t<title>Document<\/title>\n\t<style>\n\t\t.featured-collection {\n\t\t\tpadding-bottom: 0;\n\t\t\tpadding-top: 110px;\n\t\t}\n\n\t\t#shopify-section-template--16972047712432__section_custom_html_b76kkH {\n\t\t\tz-index: 999;\n\t\t\tposition: sticky;\n\t\t\ttop: 0px;\n\t\t\tleft: 0;\n\t\t}\n\n\t\t.pc-page-tab {\n\t\t\tz-index: 999;\n\t\t\tposition: sticky;\n\t\t\ttop: 0px;\n\t\t\tleft: 0;\n\t\t\twidth: 100%;\n\t\t\tbackground: #fff;\n    \tborder-bottom: 1px solid rgba(0, 0, 0, .08);\n\t\t\tdisplay: block;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content::after {\n\t\t\tcontent: none;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content {\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t\tjustify-content: space-between;\n\t\t\theight: 50px;\n\t\t}\n\n\t\t.pc-page-tab-content .pc-page-tab-content-left {\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t\tflex-wrap: nowrap;\n\t\t\twidth: 100%;\n\t\t\theight: 100%;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .pc-page-tab-content-left>div {\n\t\t\twidth: calc(100% \/ 6);\n\t\t\tmargin: 0 15px;\n\t\t\theight: 50px;\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t\tjustify-content: center;\n\t\t\ttext-align: center;\n\t\t\tborder-bottom: 2px solid transparent;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .pc-page-tab-content-right {\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t\tflex-wrap: nowrap;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .colorStyle {\n\t\t\tmargin-right: 40px;\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .bottom-button-wrapper {\n\t\t\tdisplay: flex;\n\t\t\t\/* width: 350px; *\/\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .bottom-button-wrapper>div {\n\t\t\tdisplay: flex;\n\t\t\talign-items: center;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .bottom-button-wrapper button {\n\t\t\tmargin: 0 6px;\n\t\t\tpadding: 6px 0;\n\t\t\tborder-radius: 4px !important;\n\t\t\twidth: 150px;\n\t\t\tmin-width: auto;\n\t\t}\n\n\t\t.pc-page-tab .pc-page-tab-content .colorStyle .mr10 {\n\t\t\tmargin-right: 10px;\n\t\t}\n\n\t\t.pc-page-tab-button {\n\t\t\tfont-size: 14px;\n\t\t\tcursor: pointer;\n\t\t}\n\n\t\t.pc-page-tab .active {\n\t\t\tfont-weight: 700;\n\t\t\tborder-bottom: 2px solid #171719!important;\n\t\t}\n\n\t\t.btnTab {\n\t\t\tdisplay: none;\n\t\t}\n\n\t\t@media only screen and (max-width: 1024px) {\n\t\t\t#shopify-section-template--16972047712432__section_custom_html_b76kkH {\n\t\t\t\ttop: 20px;\n\t\t\t}\n\n\t\t\t.pc-page-tab-content .pc-page-tab-content-left {\n\t\t\t\toverflow-x: auto;\n\t\t\t\toverflow-y: hidden;\n\t\t\t}\n\n\t\t\t.pc-page-tab .pc-page-tab-content .pc-page-tab-content-left>div {\n\t\t\t\tflex: 0 0 33.33%;\n\t\t\t\tmargin: 0;\n\t\t\t\tfont-size: 12px;\n\t\t\t}\n\n\t\t\t.pc-page-tab .pc-page-tab-content {\n\t\t\t\tposition: relative;\n\t\t\t}\n\n\t\t\t.btnTab {\n\t\t\t\tdisplay: block;\n\t\t\t\tposition: absolute;\n\t\t\t\ttop: 50%;\n\t\t\t\ttransform: translateY(-50%);\n\t\t\t\tpadding: 10px;\n\t\t\t\tbackground-color: #f5f5f5;\n\t\t\t\tcolor: #171719;\n\t\t\t\tborder: none;\n\t\t\t\tcursor: pointer;\n\t\t\t\tz-index: 1;\n\t\t\t\twidth: 10px;\n\t\t\t\tdisplay: flex;\n\t\t\t\tjustify-content: center;\n\t\t\t}\n\n\t\t\t.btn-left {\n\t\t\t\tleft: 0;\n\t\t\t}\n\n\t\t\t.btn-right {\n\t\t\t\tright: 0;\n\t\t\t}\n\n\t\t\t.pc-page-tab-content-left::-webkit-scrollbar {\n\t\t\t\tdisplay: none;\n\t\t\t}\n\t\t}\n\t<\/style>\n<\/head>\n\n<body>\n\t<div class=\"page-tab\">\n\t\t<div class=\"pc-page-tab\">\n\t\t\t<div class=\"pc-page-tab-content wrapper\">\n\t\t\t\t<div class=\"pc-page-tab-content-left\">\n\t\t\t\t\t<div class=\"pc-page-tab1 pc-page-tab-button active\" onclick=\"handlePreviewClick()\">\n\t\t\t\t\t\tDRESSES\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab2 pc-page-tab-button\" onclick=\"handlePreviewClick2()\">\n\t\t\t\t\t\tBODYSUITS\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab3 pc-page-tab-button\" onclick=\"handlePreviewClick3()\">\n\t\t\t\t\t\tJUMPSUITS\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab4 pc-page-tab-button\" onclick=\"handlePreviewClick4()\">\n\t\t\t\t\t\tSWIMWEAR\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab5 pc-page-tab-button\" onclick=\"handlePreviewClick5()\">\n\t\t\t\t\t\tACTIVEWEAR\n\t\t\t\t\t<\/div>\n\t\t\t\t\t<div class=\"pc-page-tab6 pc-page-tab-button\" onclick=\"handlePreviewClick6()\">\n\t\t\t\t\t\tBLUETAG\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<button class=\"btnTab btn-left\">&#10094;<\/button>\n\t\t\t\t<button class=\"btnTab btn-right\">&#10095;<\/button>\n\t\t\t<\/div>\n\t\t<\/div>\n\t<\/div>\n\t<script>\n\t\tconst windowWidth = window.innerWidth\n\t\tconst content = document.querySelector('.pc-page-tab-content-left');\n\t\tconst btnLeft = document.querySelector('.btn-left');\n\t\tconst btnRight = document.querySelector('.btn-right');\n\n\t\tbtnLeft.addEventListener('click', () => {\n\t\t\tcontent.scrollBy({\n\t\t\t\tleft: -windowWidth,\n\t\t\t\tbehavior: 'smooth'\n\t\t\t});\n\t\t});\n\n\t\tbtnRight.addEventListener('click', () => {\n\t\t\tcontent.scrollBy({\n\t\t\t\tleft: windowWidth,\n\t\t\t\tbehavior: 'smooth'\n\t\t\t});\n\t\t});\n\t\tconst scrollToElement = (selector) => {\n\t\t\tconst element = document.querySelector(selector);\n\t\t\tif (element) {\n\t\t\t\telement.scrollIntoView({\n\t\t\t\t\tbehavior: 'smooth',\n\t\t\t\t\tblock: 'start',\n\t\t\t\t\tinline: 'start'\n\t\t\t\t});\n\t\t\t} else {\n\t\t\t\tconsole.error('not found.');\n\t\t\t}\n\t\t};\n\n\n\t\tconst handlePreviewClick = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_gC4TXp');\n\t\t};\n\n\t\tconst handlePreviewClick2 = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_W6pxVQ');\n\t\t};\n\t\tconst handlePreviewClick3 = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_qFVHWD');\n\t\t};\n\t\tconst handlePreviewClick4 = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_m3QKqB');\n\t\t};\n\t\tconst handlePreviewClick5 = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_MC796c');\n\t\t};\n\t\tconst handlePreviewClick6 = () => {\n\t\t\tscrollToElement('#shopify-section-template--16972047712432__section_collection_zA7Ega');\n\t\t};\n\n\t\t\/\/ 监听页面滚动事件\n\t\twindow.addEventListener('scroll', () => {\n\n\t\t\tvar TabPos = document.querySelector('#shopify-section-template--16972047712432__section_collection_gC4TXp')?.offsetTop - 300;\n\t\t\tvar TabPos2 = document.querySelector('#shopify-section-template--16972047712432__section_collection_W6pxVQ')?.offsetTop - 300;\n\t\t\tvar TabPos3 = document.querySelector('#shopify-section-template--16972047712432__section_collection_qFVHWD')?.offsetTop - 300;\n\t\t\tvar TabPos4 = document.querySelector('#shopify-section-template--16972047712432__section_collection_m3QKqB')?.offsetTop - 300;\n\t\t\tvar TabPos5 = document.querySelector('#shopify-section-template--16972047712432__section_collection_MC796c')?.offsetTop - 300;\n\t\t\tvar TabPos6 = document.querySelector('#shopify-section-template--16972047712432__section_collection_zA7Ega')?.offsetTop - 300;\n\n\t\t\tvar scrollPos = window.scrollY;\n\t\t\tif (scrollPos >= TabPos && scrollPos < TabPos2) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab1').classList.add('active');\n\t\t\t} else if (scrollPos >= TabPos2 && scrollPos < TabPos3) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab2').classList.add('active');\n\t\t\t} else if (scrollPos >= TabPos3 && scrollPos < TabPos4) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab3').classList.add('active');\n\t\t\t} else if (scrollPos >= TabPos4 && scrollPos < TabPos5) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab4').classList.add('active');\n\t\t\t} else if (scrollPos >= TabPos5 && scrollPos < TabPos6) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab5').classList.add('active');\n\t\t\t} else if (scrollPos >= TabPos6) {\n\t\t\t\tdocument.querySelectorAll('.pc-page-tab-button').forEach(tab => {\n\t\t\t\t\ttab.classList.remove('active');\n\t\t\t\t});\n\t\t\t\tdocument.querySelector('.pc-page-tab6').classList.add('active');\n\t\t\t}\n\n\t\t});\n\t<\/script>\n<\/body>\n\n<\/html>",

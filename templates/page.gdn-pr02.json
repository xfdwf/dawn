        "heading_aHFYgf": {
          "type": "heading",
          "settings": {
            "text": "<p>Popilush brand and products have garnered widespread <br\/>attention in major\/top-tier media<\/p>",
            "size": 100,
            "heading_highlight_type": "italics",
            "highlight_color": "#d43747",
            "highlight_text_color": "#000000"

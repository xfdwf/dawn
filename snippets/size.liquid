<style>
    .size-chart-pl-main table {
        border-collapse: collapse;
        border-spacing: 0;
        border: 1px solid #e9e9e9;
        table-layout: auto;
    }

    .size-chart-pl-main td {
        text-align: center;
        padding: 10px 0;
        border-bottom: 1px solid #e9e9e9;
        font-size: 12px;
    }

    .size-chart-pl-main .no-bottom {
        border-bottom: none;
    }

    .size-chart-pl-main th {
        background: #F6F6F6;
        padding: 10px 0px;
        border-bottom: 1px solid #e9e9e9;
        font-size: 12px;
        text-align: center;
        min-width: 65px;
    }

    .size-chart-pl-main {
        text-align: left;
    }

    .size-chart-pl-main .no-bottom {
        border-bottom: none;
    }

    .size-icon {
        width: 40px;
        height: auto;
        margin-right: 10px;
    }

    .dec-first {
        color: #7D4F42;
        margin-bottom: 0;
        margin-top: 10px;
        text-align: left;
        display: flex;
        align-items: center;
        background-color: #f5efec;
        padding: 10px;
        border-radius: 8px;
    }

    .dec-second {
        color: #7D4F42;
        margin-bottom: 0;
        margin-top: 10px;
        text-align: left;
        display: flex;
        align-items: center;
        background-color: #f6f6f6;
        padding: 10px;
        border-radius: 8px;
    }

    .borderRight {
        border-right: 1px solid #e9e9e9;
    }

    @media screen and (max-width:767px) {
        .size-icon {
            width: 30px;
            height: auto;
            margin-right: 10px;
        }
    }
    .return60-size {
        width: 100%;
        max-width: none;
        margin-top: 15px;
        background: rgb(236, 236, 236);
    }

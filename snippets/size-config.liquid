  {% assign res_items = tag | append: tag_1 | append: tag_2  %}

  {% assign res_items = res_items | split: '^' %}

  {% for beatles1 in res_items %}
    {% if beatles1 != blank %}
      {% assign item = beatles1 | split: '|' %}
      {% if tag_size == item.first %}
        {% assign conten = item.last %}
        {% assign beatles = conten | split: '+' %}
        {% for item in beatles %}
          {% assign table = item | split: '%' %}
          <table class="contained" style="width: 100% !important;margin-bottom: 10px;">
            {% for part in table %}
              {% assign index = forloop.index0 %}
              {% case index %}
                {% when 0 %}
                  {% if part != blank %}
                    <thead>
                      <tr>
                        <th scope="col" colspan="8">{{ part }}</th>
                      </tr>
                    </thead>
                  {% endif %}
                {% when 1 %}
                  <thead>
                    <tr>
                      {% assign head = part | split: ',' %}
                      {% for item in head %}
                        <th scope="col">{{ item }}</th>
                      {% endfor %}
                    </tr>
                  </thead>
                {% when 2 %}
                  {% assign body = part | split: '*' %}
                  <tbody>
                    {% for tr in body %}
                      {% assign td = tr | split: ',' %}
                      <tr>
                        {% for tditem in td %}
                          {% assign tdindex = forloop.index0 %}

                          {% if tdindex == 0 %}
                            <th scope="row">{{ tditem }}</th>
                          {% else %}
                            <td>{{ tditem }}</td>
                          {% endif %}
                        {% endfor %}
                      </tr>
                    {% endfor %}
                  </tbody>
                {% else %}

              {% endcase %}
            {% endfor %}
          </table>
        {% endfor %}
      {% endif %}
    {% endif %}
  {% endfor %}
</div>
